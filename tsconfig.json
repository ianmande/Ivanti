{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "useDefineForClassFields": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "resolveJsonModule": true,
    "target": "ESNEXT",
    "baseUrl": "./",
    "paths": {
      "*": ["src/*", "*"]
    }
  }
}
